$LimitedColumns | Group-Object -Property @{Expression={$_.IsLoginDisabled + ', ' + $_.IsPasswordPolicyChecked + ', ' + $_.IsPasswordExpirationChecked}} | ForEach-Object {
    Write-Host "Logins with: $($_.Group[0].IsLoginDisabled), $($_.Group[0].IsPasswordPolicyChecked), $($_.Group[0].IsPasswordExpirationChecked)}"
    foreach ($login in $_.Group) {
        Write-Host "  - $($login.LoginName)"
    }
}


nxt
$groupedData = $LimitedColumns | Group-Object -Property @{Expression={$_.IsLoginDisabled + ', ' + $_.IsPasswordPolicyChecked + ', ' + $_.IsPasswordExpirationChecked}}
$groupedData | ForEach-Object {
    $_ | Export-Csv -Path "output_$($_.Group[0].IsLoginDisabled)_$($_.Group[0].IsPasswordPolicyChecked)_$($_.Group[0].IsPasswordExpirationChecked).csv" -NoTypeInformation
}
